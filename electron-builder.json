{
  "productName": "bcwebdiff",
  "buildVersion": "latest",
  "asar": false,
  "publish": ["github"],
  "directories": {
    "app": "dist",
    "output": "app-builds"
  },
  "win": {
    "icon": "dist/assets/icons/favicon",
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      }
    ]
  },
  "mac": {
    "icon": "dist/assets/icons/favicon",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/assets/icons",
    "desktop": {
      "Name": "Website DIFF"
    },
    "category": "Development",
    "target": [
      "AppImage",
      "deb"
    ]
  }
}
